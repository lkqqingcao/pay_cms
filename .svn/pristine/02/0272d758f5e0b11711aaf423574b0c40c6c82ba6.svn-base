package com.mobogenie.pay.auth.domain;

/**
 * 
 * @author wanghongfeng
 * @date 2014年7月12日
 */
public class Resource {
	private Integer  id;
	private String   description;
	private String   name;
	private String   pattern;
	private int      isDelete; //0正常1删除
	private Integer  parentId;
	private Integer  level;
	private String   path;
	private Boolean  hasit;
	
	
	public Boolean getHasit() {
		return hasit;
	}
	public void setHasit(Boolean hasit) {
		this.hasit = hasit;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getPattern() {
		return pattern;
	}
	public void setPattern(String pattern) {
		this.pattern = pattern;
	}
	public Integer getParentId() {
		return parentId;
	}
	public void setParentId(Integer parentId) {
		this.parentId = parentId;
	}
	public Integer getLevel() {
		return level;
	}
	public void setLevel(Integer level) {
		this.level = level;
	}
	public String getPath() {
		return path;
	}
	public void setPath(String path) {
		this.path = path;
	}
	public int getIsDelete() {
		return isDelete;
	}
	public void setIsDelete(int isDelete) {
		this.isDelete = isDelete;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Override
    public String toString() {
	    return "Resource [id=" + id + ", description=" + description + ", name=" + name
	            + ", pattern=" + pattern + ", isDelete=" + isDelete + ", parentId=" + parentId
	            + ", level=" + level + ", path=" + path + ", hasit=" + hasit + "]";
    }

}
